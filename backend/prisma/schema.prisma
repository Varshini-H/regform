// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init
generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}


datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model employee{
  firstname String
  lastname String
  employeeId String @id @unique()
  email String @unique()
  phone String
  department String
  dateOfJoining DateTime
  role String
  salary String
}
